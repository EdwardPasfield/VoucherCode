{
  "name": "vouchercodes",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": false,
  "devDependencies": {
    "cross-env": "5.2.0",
    "cypress": "8.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.0",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.0"
  },
  "scripts": {
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "generate-report": "marge cypress/reports/mochareports/report.json --reportFilename=Report --timestamp=yyyy-mm-dd -o cypress/reports/mochareports",
    "merge-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "posttest": "npm run merge-reports && npm run generate-report",
    "test": "npm run e2e-headless:vc || npm run posttest",
    "cypress": "cypress open",
    "e2e": "cypress open",
    "e2e:vc": "cross-env HOST=https://vouchercodes.co.uk/ npm run e2e",
    "e2e-headless": "cypress run",
    "e2e-headless:vc": "cross-env HOST=https://vouchercodes.co.uk/ npm run e2e-headless"
  },
  "description": "End to End tests for voucher coders",
  "author": "Ed Pasfield"
}
